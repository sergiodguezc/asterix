pot solucion {
    intix valor = 3;
    vectix<intix>[3] V = {1,2,3};
};

potion changeValue(valor : intix, sol : solucion) -> solucion {
    sol.valor = valor;
    sol.V = {5,6,7};
    return sol;
}

potion panoramix() -> intix {
    solucion sol;
    solucion sol2;
    sol2 = changeValue(4,sol);

    forix(intix elem : sol.V) {
        stilus(elem);
    }
    stilus(sol.valor);

    forix(intix elem : sol2.V) {
        stilus(elem);
    }

    stilus(sol2.valor);

    return 0;
}
