potion panoramix() -> intix {
    vectix<vectix<intix>[3]>[3] v = {{1,2,3},{4,5,6},{7,8,9}};

    forix (vectix<intix>[3] fila : v) {
        forix (intix elem : fila) {
            tabellae(elem^2);
        }
    }

    vectix<vectix<intix[3]>[3] v2 = v

    forix (vectix<intix>[3] fila : v2) {
        forix (intix elem : fila) {
